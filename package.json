{
	"name": "insight",
	"version": "1.0.0",
	"main": "src/index.ts",
	"license": "MIT",
	"scripts": {
		"start": "cross-env NODE_ENV=production npx ts-node src/index.ts",
		"dev": "cross-env NODE_ENV=development nodemon --watch src --exec  npx ts-node src/index.ts",
		"build": "tsc",
		"typeorm": "typeorm-ts-node-commonjs -d src/db/data-source.ts",
		"migration:show": "yarn typeorm migration:show",
		"migration:create": "node scripts/migration-create.js",
		"migration:run": "yarn typeorm migration:run",
		"migration:revert": "yarn typeorm migration:revert",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"node",
		"typescript",
		"express",
		"mysql",
		"typeorm"
	],
	"author": "taurusilver7",
	"repository": {
		"type": "git",
		"url": "https://github.com/taurusilver7/"
	},
	"dependencies": {
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"helmet": "^7.1.0",
		"morgan": "^1.10.0",
		"mysql2": "^3.9.2",
		"reflect-metadata": "^0.2.1",
		"typeorm": "^0.3.20"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.22",
		"cross-env": "^7.0.3",
		"nodemon": "^3.1.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3",
		"yargs": "^17.7.2"
	}
}
